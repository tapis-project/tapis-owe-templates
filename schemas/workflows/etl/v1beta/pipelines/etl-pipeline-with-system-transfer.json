{
    "description": "ETL Pipeline",
    "execution_profile": {
        "duplicate_submission_policy": "deny"
    },
    "params": {
        "TAPIS_BASE_URL": {
            "type": "string",
            "required": true
        },
        "TAPIS_USERNAME": {
            "type": "string",
            "required": true
        },
        "TAPIS_PASSWORD": {
            "type": "string",
            "required": true
        },
        "TAPIS_JWT": {
            "type": "string",
            "required": false
        },
        "RESUBMIT_INGRESS": {
            "type": "string",
            "required": false
        },
        "RESUBMIT_TRANSFORM": {
            "type": "string",
            "required": false
        },
        "RESUBMIT_EGRESS": {
            "type": "string",
            "required": false
        }
    },
    "env": {
        "TAPIS_SYSTEM_FILE_REF_EXTENSION": {
            "type": "string",
            "value": "TapisSystemFile.json"
        }
    },
    "tasks": [
        {
            "id": "prepare-systems",
            "description": "Create directories required by an etl pipeline on all systems",
            "type": "template",
            "uses": {
                "name": "tapis/etl/prepare-systems@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [],
            "conditions": [
                {
                    "ne": [
                        {"args": "SKIP_PREPARE_SYSTEMS"},
                        true
                    ]
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "TAPIS_JWT": {
                    "required": false,
                    "value_from": {
                        "args": "TAPIS_JWT"
                    }
                },
                "LOCAL_INBOX": {
                    "value_from": {
                        "env": "LOCAL_INBOX"
                    }
                },
                "LOCAL_OUTBOX": {
                    "value_from": {
                        "env": "LOCAL_OUTBOX"
                    }
                },
                "REMOTE_INBOX": {
                    "value_from": {
                        "env": "REMOTE_INBOX"
                    }
                },
                "REMOTE_OUTBOX": {
                    "value_from": {
                        "env": "REMOTE_OUTBOX"
                    }
                }
            }
        },
        {
            "id": "pull-gen-remote-outbox-manifests",
            "description": "Transfer manifests from remote outbox to remote inbox",
            "type": "template",
            "uses": {
                "name": "tapis/etl/pull-gen-manifests@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [
                {
                    "id": "prepare-systems",
                    "can_fail": false,
                    "can_skip": true
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "TAPIS_JWT": {
                    "required": false,
                    "value_from": {
                        "args": "TAPIS_JWT"
                    }
                },
                "INGRESS_SYSTEM": {
                    "value_from": {
                        "env": "LOCAL_INBOX"
                    }
                },
                "EGRESS_SYSTEM": {
                    "value_from": {
                        "env": "REMOTE_OUTBOX"
                    }
                }
            }
        },
        {
            "id": "ingest-remote-outbox-data",
            "description": "Transfer data files from the remote outbox to the local inbox",
            "type": "template",
            "uses": {
                "name": "tapis/etl/push-pull-data@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [
                {
                    "id": "pull-gen-remote-outbox-manifests",
                    "can_fail": false,
                    "can_skip": false
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "TAPIS_JWT": {
                    "required": false,
                    "value_from": {
                        "args": "TAPIS_JWT"
                    }
                },
                "INGRESS_SYSTEM": {
                    "value_from": {
                        "env": "LOCAL_INBOX"
                    }
                },
                "EGRESS_SYSTEM": {
                    "value_from": {
                        "env": "REMOTE_OUTBOX"
                    }
                },
                "PHASE": {
                    "value": "ingress"
                }
            }
        },
        {
            "id": "select-transform-manifest",
            "description": "Outputs the next manifest to be processed",
            "type": "template",
            "uses": {
                "name": "tapis/etl/select-transform-manifest@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [
                {
                    "id": "ingest-remote-outbox-data",
                    "can_fail": false,
                    "can_skip": false
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "LOCAL_INBOX": {
                    "value_from": {
                        "env": "LOCAL_INBOX"
                    }
                },
                "RESUBMIT_TRANSFORM": {
                    "required": false,
                    "value_from": {
                        "args": "RESUBMIT_TRANSFORM"
                    }
                },
                "TAPIS_SYSTEM_FILE_REF_EXTENSION": {
                    "value_from": {
                        "env": "TAPIS_SYSTEM_FILE_REF_EXTENSION"
                    }
                }
            },
            "conditions": [
                {
                    "and": [
                        {
                            "eq": [
                                {"args": "RESUBMIT_INGRESS"},
                                null
                            ]
                        },
                        {
                            "eq": [
                                {"args": "RESUBMIT_EGRESS"},
                                null
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "id": "submit-tapis-jobs",
            "description": "Submits all tapis jobs",
            "type": "template",
            "uses": {
                "name": "tapis/etl/run-mainfest-jobs@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [
                {
                    "id": "select-transform-manifest",
                    "can_fail": false,
                    "can_skip": false
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "LOCAL_INBOX": {
                    "value_from": {
                        "env": "LOCAL_INBOX"
                    }
                },
                "DEFAULT_ETL_JOBS": {
                    "value_from": {
                        "env": "DEFAULT_ETL_JOBS"
                    }
                },
                "MANIFEST": {
                    "value_from": {
                        "task_output": {
                            "task_id": "select-transform-manifest",
                            "output_id": "MANIFEST"
                        }
                    }
                },
                "TAPIS_SYSTEM_FILE_REF_EXTENSION": {
                    "value_from": {
                        "env": "TAPIS_SYSTEM_FILE_REF_EXTENSION"
                    }
                }
            },
            "conditions": [
                {
                    "eq": [
                        {"args": "RESUBMIT_EGRESS"},
                        null
                    ]
                }
            ]
        },
        {
            "id": "gen-local-outbox-manifests",
            "description": "Generates a manifests for output data files in data directory of the local outbox system.",
            "type": "template",
            "uses": {
                "name": "tapis/etl/gen-local-outbox-manifests@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "depends_on": [
                {
                    "id": "submit-tapis-jobs",
                    "can_skip": true
                }
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "EGRESS_SYSTEM": {
                    "value_from": {
                        "env": "LOCAL_OUTBOX"
                    }
                },
                "INGRESS_SYSTEM": {
                    "value_from": {
                        "env": "REMOTE_INBOX"
                    }
                },
                "RESUBMIT_EGRESS": {
                    "value_from": {
                        "args": "RESUBMIT_EGRESS"
                    }
                }
            },
            "conditions": []
        },
        {
            "id": "tapis-transfer-results",
            "type": "template",
            "uses": {
                "name": "tapis/etl/push-pull-data@v1beta",
                "source": {
                    "url": "https://github.com/tapis-project/tapis-workflows-task-templates.git",
                    "branch": "master"
                }
            },
            "conditions": [],
            "depends_on": [
                {"id": "gen-local-outbox-manifests"}
            ],
            "input": {
                "TAPIS_BASE_URL": {
                    "value_from": {
                        "args": "TAPIS_BASE_URL"
                    }
                },
                "TAPIS_USERNAME": {
                    "value_from": {
                        "args": "TAPIS_USERNAME"
                    }
                },
                "TAPIS_PASSWORD": {
                    "value_from": {
                        "args": "TAPIS_PASSWORD"
                    }
                },
                "EGRESS_SYSTEM": {
                    "value_from": {
                        "env": "LOCAL_OUTBOX"
                    }
                },
                "INGRESS_SYSTEM": {
                    "value_from": {
                        "env": "REMOTE_INBOX"
                    }
                },
                "PHASE": {
                    "value": "egress"
                },
                "RESUBMIT_EGRESS": {
                    "value_from": {
                        "args": "RESUBMIT_EGRESS"
                    }
                }
            }
        }
    ]
}